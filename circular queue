 #include<stdio.h>
#define max 10
int Queue[max];
int front=-1;
int rear=-1;
void Enqueue(int value)
{
    if((rear==max-1 && front == 0) || front == rear+1)
    {
        printf("Over Flow\n");
    }
    else
    {
        if(front==-1)
        {
            front=0;
            rear=0;
            Queue[rear]=value;
        }
        else
        {
            rear=(rear+1)%max;
            Queue[rear]=value;
        }
    }
}
void Dequeue()
{
    if(front == -1 && rear == -1)
    {
        printf("UnderFlow\n");
    }
    else
    {
        if(front == rear)
        {
            front = rear = -1;
        }
        else
        {
            front = (front+1)%max;
        }
    }
}
void Display()
{
     if(front == -1 && rear == -1)
    {
        printf("List Empty\n");
    }
    else
    {
        int i;
        for(i=front;i!=rear;i=(i+1)%max)
        {
            printf("%d ",Queue[i]);
        }
        printf("%d ",Queue[i]);
    }
}
int main()
{
    Enqueue(1);
    Enqueue(2);
    Enqueue(3);
    printf("\n After Enqueued :");
    Display();
    printf("\n After Dequeued :");
    Dequeue();
    Display();
    return 0;
}
